print("Welcome to my learning quiz !")

playing = input("Do you want to play? ")

if playing != "yes":
    quit()
    
print("Okay! Let's play:) ")
score = 0

answer = input("What does CPU stand for? ")
if answer == "central processing unit":
    print('Correct!')
    score +=1
else:
    print('Incorrect!')
    
    answer = input("What does GPU stand for? ")
if answer.lower()== "grapical processing unit":
    print('Correct!')
    score +1
else:
    print('Incorrect!')
    
    answer = input("What does RAM stand for? ")
if answer.lower() == "random access memory":
    print('Correct!')
    score +=1
else:
    print('Incorrect!')
    
    answer = input("What does PSU stand for? ")
if answer.lower() == "Power Supply":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
    
    answer = input("What does FHNW stand for? ")
if answer.lower()== "Northwestern University Windisch":
    print('Correct!')
    score += 1
else:
    print('Incorrect!')
